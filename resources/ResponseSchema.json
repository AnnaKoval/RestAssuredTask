{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": [
    {
      "type": "object",
      "properties": {
        "self": {
          "type": "string"
        },
        "id": {
          "type": "string"
        },
        "filename": {
          "type": "string"
        },
        "author": {
          "type": "object",
          "properties": {
            "self": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "key": {
              "type": "string"
            },
            "accountId": {
              "type": "string"
            },
            "emailAddress": {
              "type": "string"
            },
            "avatarUrls": {
              "type": "object",
