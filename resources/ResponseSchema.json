                "48x48": {
                  "type": "string"
                },
                "32x32": {
                  "type": "string"
                }
              },
              "required": [
                "48x48",
                "32x32"
              ]
            },
            "displayName": {
              "type": "string"
            },
            "active": {
              "type": "boolean"
            },
            "timeZone": {
              "type": "string"
            },
            "accountType": {
              "type": "string"
            }
          },
          "required": [
            "self",
            "name",
            "key",
            "accountId",
            "emailAddress",
            "avatarUrls",
            "displayName",
            "active",
            "timeZone",
            "accountType"
          ]
        },
        "created": {
          "type": "string"
        },
        "size": {
          "type": "integer"
        },
        "mimeType": {
          "type": "string"
        },
        "content": {
          "type": "string"
        },
        "thumbnail": {
          "type": "string"
        }
      },
      "required": [
        "self",
        "id",
        "filename",
        "author",
        "created",
        "size",
        "mimeType",
        "content",
        "thumbnail"
      ]
    }
  ]
